{
  "name": "opendevbrowser-frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "sync:assets": "node scripts/sync-brand-assets.mjs",
    "generate:docs": "node scripts/generate-docs.mjs",
    "predev": "npm run sync:assets && npm run generate:docs",
    "dev": "rm -rf .next && next dev",
    "prebuild": "npm run sync:assets && npm run generate:docs",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "rm -rf .next .next-dev 2>/dev/null || true && rm -rf .next .next-dev 2>/dev/null || true && next typegen && tsc --noEmit"
  },
  "dependencies": {
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "next": "^15.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "three": "^0.170.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.27",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "eslint": "^9.12.0",
    "eslint-config-next": "^15.0.0",
    "typescript": "^5.9.3"
  }
}
